    uint256 public constant FEE_DENOMINATOR = 1000;

    uint256 public buyTax;
    uint256 public sellTax;

    uint256 public buyLimit;
    uint256 public sellLimit;
    uint256 public transferLimit;

    uint256 public nextTransactionTimeGap = 2 minutes;

    address public feeReceiver;

    constructor() ERC20(_name, _symbol) Ownable(_msgSender()) {
        _totalSupply = 1000000000 * 10**decimals();

        _mint(_msgSender(), _totalSupply);

        buyTax = 2;
        sellTax = 5;

        buyLimit = 5;
        sellLimit = 7;
        transferLimit = 10;

        feeReceiver = _msgSender();
    }

    function isContract(address _addr) private view returns (bool) {
        uint32 size;
        assembly {
            size := extcodesize(_addr)
        }
        return (size > 0);
    }

    modifier limitProtect(
        address sender,
        address recipient,
        uint256 amount
    ) {
        uint256 limit = transferLimit;
        string memory _type = "transfer";

        if (isContract(sender)) {
            limit = buyLimit;
            _type = "buy";
        } else if (isContract(recipient)) {
            limit = sellLimit;
            _type = "sell";
        } else {
            limit = transferLimit;
            _type = "transfer";
        }

        require(
            limit >= amount,
            string(
                abi.encode(
                    "The transfer amount of ",
                    amount,
                    " exceeds the allowed ",
                    _type,
                    " limit. The maximum allowed amount is ",
                    limit
                )
            )
        );

        _;
    }

    receive() external payable {}

    function updateTransactionTimeLimit(uint256 _seconds) external onlyOwner {
        nextTransactionTimeGap = _seconds;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        limitProtect(msg.sender, recipient, amount)
        returns (bool)
    {
        require(amount > 0, "Invalid amount");
        require(recipient != address(0), "Invalid recipient");

        return _transferFrom(_msgSender(), recipient, amount);
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        require(amount > 0, "Invalid amount");
        require(recipient != address(0), "Invalid recipient");
        require(sender != address(0), "Invalid sender");

        uint256 currentAllowance = allowance(sender, _msgSender());
        require(currentAllowance >= amount, "Insufficient Allowance");

        return _transferFrom(sender, recipient, amount);
    }

    function _transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) internal limitProtect(sender, recipient, amount) returns (bool) {
        uint256 senderBalance = balanceOf(sender);
        require(senderBalance >= amount, "Insufficient Balance");

        uint256 amountReceived = takeFee(sender, amount);
        _transfer(sender, recipient, amountReceived);

        return true;
    }

    function takeFee(address sender, uint256 amount)
        internal
        returns (uint256)
    {
        uint256 fee = sender == _msgSender() ? sellTax : buyTax;

        uint256 feeAmount = amount.mul(fee).div(FEE_DENOMINATOR);
        uint256 transferable_amount = amount.sub(feeAmount);

        uint256 senderBalance = balanceOf(sender);
        require(senderBalance >= amount, "Insufficient Balance");
        _transfer(sender, feeReceiver, feeAmount);

        return transferable_amount;
    }

    function manualSend() external {
        require(
            _msgSender() == feeReceiver,
            "only fee receiver can transfer funds"
        );
        payable(feeReceiver).transfer(address(this).balance);
    }

    function clearStuckToken(address tokenAddress, uint256 tokens)
        external
        returns (bool success)
    {
        require(
            _msgSender() == feeReceiver,
            "only fee receiver can transfer funds"
        );

        if (tokens == 0) {
            tokens = ERC20(tokenAddress).balanceOf(address(this));
        }
        return ERC20(tokenAddress).transfer(feeReceiver, tokens);
    }

    function setFee(uint256 _buy, uint256 _sell) external onlyOwner {
        buyTax = _buy;
        sellTax = _sell;
    }

    function setFeeReceiver(address _feeReceiver) external {
        require(
            _msgSender() == feeReceiver,
            "You are not allowed to set fee receiver"
        );

        feeReceiver = _feeReceiver;
    }
}
